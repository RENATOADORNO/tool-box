    name = Renato Adorno
    email = renattoadorno@gmail.com
    # signingKey =
[github]
    user = renatoadorno
[core]
    editor = code
[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
    ui = true
[color "status"]
    added = green
    changed = yellow
    untracked = cyan
    removed = red
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[alias]
    fc = !git add . && git commit -m 'ðŸŽ‰ initial commit'
    cmt = commit -m
    ck = checkout
    cm = checkout main
    cb = checkout -b
    ca = commit --amend --no-edit
    rc = commit --amend -m
    rb = branch -m
    st = status -sb
    sf = show --name-only
    lg = log --pretty=format:'%Cred%h%Creset %C(bold)%cr%Creset %Cgreen<%an>%Creset %s' --max-count=30
    incoming = !(git fetch --quiet && git log --pretty=format:'%C(yellow)%h %C(white)- %C(red)%an %C(white)- %C(cyan)%d%Creset %s %C(white)- %ar%Creset' ..@{u})
    outgoing = !(git fetch --quiet && git log --pretty=format:'%C(yellow)%h %C(white)- %C(red)%an %C(white)- %C(cyan)%d%Creset %s %C(white)- %ar%Creset' @{u}..)
    unstage = reset HEAD --
    undo = checkout --
    rollback = reset --soft HEAD~1
[init]
    defaultBranch = main